{
  "name": "@iamjs/react",
  "version": "0.3.12",
  "description": "This package contains React components that can be used to easily integrate the authorization library into a React application.",
  "keywords": [
    "iamjs",
    "react",
    "usePerm",
    "permissions"
  ],
  "author": "Mohamed Achaq <hi@achaq.dev>",
  "homepage": "https://iamjs.achaq.dev",
  "registry": "https://registry.npmjs.org/",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --config=./jest.config.js",
    "build": "rm -rf dist && vite build --config vite.config.js",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0"
  },
  "gitHead": "25eac252720288b823c4847328c9ac66efda0f4d",
  "dependencies": {
    "@iamjs/core": "^0.3.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
